from collections import deque
n=int(input())
dic={}
for x in range(1,n+1):
    y=int(input())
    dic[x]=y
m=0
ans=0
for i in range(1,n+1):
    deq=deque()
    check=[True]*(n+1)
    deq.append((i,1))
    check[i]=False
    while deq:
        c,k=deq.popleft()
        if check[dic[c]]:
            deq.append((dic[c],k+1))
            check[dic[c]]=False
    if k>m:
        m=k
        ans=i
print(ans)
