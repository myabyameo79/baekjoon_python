a,b=map(int,input().split())
li=[list(map(int,input().split())) for _ in range(a)]
dic={}
for i in range(a):
    for j in range(b):
        dic[li[i][j]]=dic.get(li[i][j],0)+1
odd=0
for i in dic.values():
    if i&1:odd+=1
if b&1:
    if a&1:
        if odd&1 and odd<=a:
            print('YES')
        else:print('NO')
    else:
        if odd&1:print('NO')
        else:print('YES')
else:
    if odd==0:print('YES')
    else:print('NO')
