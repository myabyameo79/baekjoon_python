t, h = [], []

n, m = map(int, input().split())

for i in range(n):
    i = list(map(int, input().split()))
    t.append(i)

for i in range(n):
    i = list(map(int, input().split()))
    h.append(i)
    
for i in range(n):
    for j in range(m):
        print(t[i][j] + h[i][j], end=' ')
    print()
