a,b=map(float,input().split())
while True:
    if a==0 and b==0:
        print('AXIS')
        break
    if a*b==0:print('AXIS')
    elif a>0 and b>0:print('Q1')
    elif a>0 and b<0:print('Q4')
    elif a<0 and b>0:print('Q2')
    elif a<0 and b<0:print('Q3')
    a, b = map(float, input().split())
