a,b=map(int,input().split())
for i in range(1,62):
    if (2**i-1)%b==0:
        m=i
        n=(2**i-1)//b
        break
if i>60:
    print(-1)
    exit()
b-=a
a*=n
b*=n
for i in range(m-1,-1,-1):
    if 2**i<=a:
        print('*',end='')
        a-=2**i
    elif 2**i<=b:
        print('-',end='')
        b-=2**i
