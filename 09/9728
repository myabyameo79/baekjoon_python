for _ in range(int(input())):
    a,b=map(int,input().split())
    li=list(map(int,input().split()))
    i=0
    j=a-1
    ans=0
    while i<j:
        if li[i]+li[j]>b:
            j-=1
        elif li[i]+li[j]<b:
            i+=1
        else:
            ans+=1
            i+=1
            j-=1
    print(f'Case #{_+1}: {ans}')
